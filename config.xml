<?xml version='1.0' encoding='utf-8'?>
<widget id="edu.uncuyo.mba" android-versionCode="3" version="2.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>MBA UNCUYO</name>
    <description>
        Esta plataforma permite el desarrollo y programaci√≥n de encuestas.
    </description>
    <author email="tecnologia@grupocore.net" href="http://grupocore.net">
        Plataforma Integral de Encuestas
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
	<access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>

	<icon src="res/logo.png" />
</widget>

